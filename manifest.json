{
    "manifest_version": 2,
    "name": "Tabulous",
    "short_name": "Tabtabtabtab",
    "version": "1.1",
    "permissions": [
      "storage"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "options_ui": {
      "page": "options.html",
      "chrome_style": false,
      "open_in_tab": true
    },
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "24": "icons/icon48.png",
        "32": "icons/icon128.png"
      },
      "default_popup": "options.html"
    }
}
